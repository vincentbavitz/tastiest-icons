{
  "name": "@tastiest-io/tastiest-icons",
  "author": "vincentbavitz",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "git://github.com:tastiest-io/tastiest-icons",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12.13"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "NODE_ENV=production npm run optimize-svg && npm run convert-svg && yarn eslint ./src/icons --fix && tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "optimize-svg": "svgo ./src/svg/*/*.svg",
    "convert-svg": "svgr --typescript -d ./src/icons/ ./src/svg"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/tastiest-icons.esm.js",
  "size-limit": [
    {
      "path": "dist/tastiest-icons.cjs.production.min.js",
      "limit": "22 KB"
    },
    {
      "path": "dist/tastiest-icons.esm.js",
      "limit": "22 KB"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@svgr/cli": "^5.5.0",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^7",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.2",
    "svgo": "^2.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
